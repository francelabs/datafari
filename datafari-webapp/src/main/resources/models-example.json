{
  "activeModel": "openai-default",
  "models": [
    {
      "name": "openai-default",
      "class": "dev.langchain4j.model.openai.OpenAiChatModel",
      "params": {
        "baseUrl": "https://api.openai.com/v1",
        "apiKey": "sk-your-openai-key",
        "modelName": "gpt-4o-mini",
        "temperature": 0.0,
        "timeout": 60,
        "logRequests": true,
        "logResponses": true,
        "maxRetries": 3
      }
    },
    {
      "name": "mistral-chat-prod",
      "class": "dev.langchain4j.model.mistralai.MistralAiChatModel",
      "params": {
        "baseUrl": "https://api.mistral.ai/v1",
        "apiKey": "sk-your-mistral-key",
        "modelName": "mistral-medium",
        "temperature": 0.1,
        "timeout": 60,
        "logRequests": true,
        "logResponses": true,
        "maxRetries": 3
      }
    },
    {
      "name": "cohere-chat-test",
      "class": "dev.langchain4j.model.cohere.CohereChatModel",
      "params": {
        "apiKey": "your-cohere-api-key",
        "modelName": "command-r-plus",
        "temperature": 0.6,
        "timeout": 45,
        "logRequests": true,
        "logResponses": false
      }
    }
  ]
}